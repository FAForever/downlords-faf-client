<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="1.7976931348623157E308" spacing="20.0" styleClass="content"
      VBox.vgrow="ALWAYS" fx:id="immediateNotificationRoot"
      xmlns="http://javafx.com/javafx/8.0.141"
      fx:controller="com.faforever.client.notification.ImmediateNotificationController">
    <children>
        <Label fx:id="notificationText" wrapText="true"/>
        <TitledPane fx:id="exceptionArea" styleClass="h3" text="%errorDetails">
            <TextArea fx:id="exceptionTextArea" editable="false" wrapText="true"/>
        </TitledPane>
        <Label fx:id="helpText" wrapText="true" text="%findHelp"/>
    </children>
    <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </VBox.margin>
</VBox>
