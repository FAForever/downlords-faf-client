<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:id="root"
      alignment="CENTER" spacing="10.0"
      fx:controller="com.faforever.client.vault.VaultEntityShowRoomController">
    <Label fx:id="label" prefWidth="${root.width}" styleClass="h2"
           text="%vault.replays.ownReplays"/>
    <FlowPane fx:id="pane" hgap="10.0" maxWidth="1.7976931348623157E308"
              prefWrapLength="1500.0" vgap="10.0"/>
    <Button fx:id="moreButton" mnemonicParsing="false" styleClass="card, hoverable, pointable"
            maxHeight="1.7976931348623157E308">
        <graphic>
            <Region styleClass="play-icon, icon"/>
        </graphic>
    </Button>
</VBox>

