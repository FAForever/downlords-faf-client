<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="root" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="com.faforever.client.vault.search.SaveQueryController" spacing="10.0">
    <TextField fx:id="queryName" maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS"/>
    <HBox alignment="CENTER" spacing="10.0">
        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onSaveButtonClicked"
                text="%saveQuery" HBox.hgrow="ALWAYS"/>
        <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onCancelButtonClicked"
                text="%cancel" HBox.hgrow="ALWAYS"/>
    </HBox>

</VBox>
