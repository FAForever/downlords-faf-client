<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import com.jfoenix.controls.JFXProgressBar?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" fx:id="ladder1v1Root" fitToWidth="true"
            maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="com.faforever.client.rankedmatch.Ladder1v1Controller">
  <content>
    <VBox alignment="CENTER" minHeight="0.0" minWidth="0.0" spacing="10.0">
      <children>
          <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="10.0" minHeight="0.0" minWidth="0.0" vgap="10.0">
          <children>
              <GridPane prefWidth="400.0" styleClass="card" vgap="10.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="NEVER" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                  <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                  <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
              </rowConstraints>
              <children>
                  <Label styleClass="h2" text="%ranked1v1.choseFactions.title" />
                  <Label text="%ranked1v1.choseFactions.description" wrapText="true" GridPane.rowIndex="1" />
                  <HBox alignment="CENTER" GridPane.rowIndex="4">
                    <children>
                      <Region HBox.hgrow="ALWAYS" />
                      <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#onCancelButtonClicked" text="%ranked1v1.cancel" />
                      <Button fx:id="playButton" defaultButton="true" mnemonicParsing="false" onAction="#onPlayButtonClicked" text="%ranked1v1.play" />
                      <Region HBox.hgrow="ALWAYS" />
                      <Label fx:id="timeUntilQueuePopLabel" />
                      <Region HBox.hgrow="ALWAYS" />
                    </children>
                  </HBox>
                <HBox alignment="CENTER" spacing="5.0" GridPane.rowIndex="2" GridPane.valignment="CENTER">
                  <children>
                      <ToggleButton fx:id="aeonButton" styleClass="faction-button" mnemonicParsing="false" onAction="#onFactionButtonClicked">
                          <graphic>
                              <Region styleClass="aeon-icon"/>
                          </graphic>
                    </ToggleButton>
                      <ToggleButton fx:id="cybranButton" styleClass="faction-button" mnemonicParsing="false" onAction="#onFactionButtonClicked">
                          <graphic>
                              <Region styleClass="cybran-icon"/>
                          </graphic>
                      </ToggleButton>
                      <ToggleButton fx:id="uefButton" styleClass="faction-button" mnemonicParsing="false" onAction="#onFactionButtonClicked">
                          <graphic>
                              <Region styleClass="uef-icon"/>
                          </graphic>
                    </ToggleButton>
                      <ToggleButton fx:id="seraphimButton" styleClass="faction-button" mnemonicParsing="false" onAction="#onFactionButtonClicked">
                          <graphic>
                              <Region styleClass="seraphim-icon"/>
                          </graphic>
                    </ToggleButton>
                  </children>
                </HBox>
                  <Label fx:id="searchingForOpponentLabel" contentDisplay="BOTTOM" text="%ranked1v1.searching" wrapText="true" GridPane.halignment="CENTER" GridPane.rowIndex="3">
                      <graphic>
                          <ImageView fx:id="searchProgressIndicator"/>
                      </graphic>
                  </Label>
              </children>
                  <padding>
                      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                  </padding>
            </GridPane>
              <GridPane hgap="10.0" prefWidth="488.0" vgap="10.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
              </rowConstraints>
              <children>
                  <GridPane prefWidth="250.0" styleClass="card">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints minHeight="10.0" vgrow="NEVER" />
                      <RowConstraints minHeight="10.0" vgrow="NEVER" />
                      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                  </rowConstraints>
                  <children>
                      <Label styleClass="h2" text="%ranked1v1.rating.title" />
                      <Label fx:id="ratingLabel" prefHeight="80.0" styleClass="h1" text="0000" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
                      <JFXProgressBar fx:id="ratingProgressIndicator" prefWidth="244.0" progress="0.66"
                                      GridPane.rowIndex="2"/>
                      <Label fx:id="ratingHintLabel" text="&lt;rating hint&gt;" wrapText="true" GridPane.rowIndex="1" />
                  </children>
                      <padding>
                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                      </padding>
                </GridPane>
                  <GridPane prefWidth="250.0" styleClass="card" GridPane.columnIndex="1">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints minHeight="10.0" vgrow="NEVER" />
                      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                      <RowConstraints minHeight="10.0" vgrow="NEVER" />
                  </rowConstraints>
                  <children>
                      <Label styleClass="h2" text="%ranked1v1.ranking" />
                      <Label fx:id="rankingLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="80.0" styleClass="h1" text="#0" GridPane.rowIndex="1" />
                      <Label fx:id="rankingOutOfLabel" text="&lt;out of #&gt;" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                  </children>
                      <padding>
                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                      </padding>
                </GridPane>
                  <GridPane prefWidth="250.0" styleClass="card" GridPane.columnIndex="1" GridPane.rowIndex="1">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints minHeight="10.0" vgrow="NEVER" />
                      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                  </rowConstraints>
                  <children>
                      <Label styleClass="h2" text="%ranked1v1.gamesPlayed" />
                      <Label fx:id="gamesPlayedLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="80.0" styleClass="h1" text="000" GridPane.rowIndex="1" />
                  </children>
                      <padding>
                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                      </padding>
                </GridPane>
                  <GridPane prefWidth="250.0" styleClass="card" GridPane.rowIndex="1">
                  <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  </columnConstraints>
                  <rowConstraints>
                      <RowConstraints minHeight="10.0" vgrow="NEVER" />
                      <RowConstraints minHeight="10.0" vgrow="ALWAYS" />
                  </rowConstraints>
                  <children>
                      <Label styleClass="h2" text="%ranked1v1.winLossRation" />
                      <Label fx:id="winLossRationLabel" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="80.0" styleClass="h1" text="0%" GridPane.rowIndex="1" />
                  </children>
                      <padding>
                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                      </padding>
                </GridPane>
              </children>
            </GridPane>
          </children>
          <padding>
              <Insets top="5.0" />
          </padding>
        </FlowPane>
          <GridPane styleClass="card" maxWidth="898">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
              </rowConstraints>
              <children>
                  <BarChart fx:id="ratingDistributionChart" categoryGap="5.0" prefHeight="300.0" prefWidth="878.0" title="%ranked1v1.rankingDistribution">
                      <xAxis>
                          <CategoryAxis fx:id="ratingDistributionXAxis" animated="false" label="%ranked1v1.rankingDistribution.x" side="BOTTOM" tickLabelGap="0.0" tickLabelRotation="-90.0" />
                      </xAxis>
                      <yAxis>
                          <NumberAxis fx:id="ratingDistributionYAxis" label="%ranked1v1.rankingDistribution.y" side="LEFT" />
                      </yAxis>
                  </BarChart>
              </children>
              <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
              </padding>
          </GridPane>
          <Button mnemonicParsing="false" onAction="#showLadderMaps" text="%ranked1v1.showMaps"/>
      </children>
      <padding>
          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </padding>
    </VBox>
  </content>
</ScrollPane>
