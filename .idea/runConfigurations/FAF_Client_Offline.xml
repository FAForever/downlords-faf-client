<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="FAF Client Offline" type="Application" factoryName="Application" singleton="true">
    <option name="MAIN_CLASS_NAME" value="com.faforever.client.FafClientApplication" />
    <module name="downlords-faf-client" />
    <option name="VM_PARAMETERS" value="-XX:+UseConcMarkSweepGC -DnativeDir=build/resources/native -Dspring.profiles.active=offline -Dprism.dirtyopts=false -Xmx512m -XX:+HeapDumpOnOutOfMemoryError" />
    <option name="WORKING_DIRECTORY" value="file://$PROJECT_DIR$" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value=".*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method />
  </configuration>
</component>